    {
      "content" : [
        {
          "anchor" : "discussion",
          "level" : 2,
          "text" : "Discussion",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "code" : "head",
              "type" : "codeVoice"
            },
            {
              "text" : " is the correctly rounded value of ",
              "type" : "text"
            },
            {
              "code" : "a*b",
              "type" : "codeVoice"
            },
            {
              "text" : ". If no overflow or",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "underflow occurs, ",
              "type" : "text"
            },
            {
              "code" : "tail",
              "type" : "codeVoice"
            },
            {
              "text" : " represents the rounding error incurred in",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "computing ",
              "type" : "text"
            },
            {
              "code" : "head",
              "type" : "codeVoice"
            },
            {
              "text" : ", such that the exact product is the sum of ",
              "type" : "text"
            },
            {
              "code" : "head",
              "type" : "codeVoice"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "and ",
              "type" : "text"
            },
            {
              "code" : "tail",
              "type" : "codeVoice"
            },
            {
              "text" : " computed without rounding.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "This operation is sometimes called “twoProd” or “twoProduct”.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "anchor" : "Edge-Cases:",
          "level" : 2,
          "text" : "Edge Cases:",
          "type" : "heading"
        },
        {
          "items" : [
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "code" : "head",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " is always the IEEE 754 product ",
                      "type" : "text"
                    },
                    {
                      "code" : "a * b",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : ".",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "If ",
                      "type" : "text"
                    },
                    {
                      "code" : "head",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " is not finite, ",
                      "type" : "text"
                    },
                    {
                      "code" : "tail",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " is unspecified and should not be",
                      "type" : "text"
                    },
                    {
                      "text" : " ",
                      "type" : "text"
                    },
                    {
                      "text" : "interpreted as having any meaning (it may be ",
                      "type" : "text"
                    },
                    {
                      "code" : "NaN",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " or ",
                      "type" : "text"
                    },
                    {
                      "code" : "infinity",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : ").",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "When ",
                      "type" : "text"
                    },
                    {
                      "code" : "head",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " is close to the underflow boundary, the rounding error",
                      "type" : "text"
                    },
                    {
                      "text" : " ",
                      "type" : "text"
                    },
                    {
                      "text" : "may not be representable due to underflow, and ",
                      "type" : "text"
                    },
                    {
                      "code" : "tail",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " will be rounded.",
                      "type" : "text"
                    },
                    {
                      "text" : " ",
                      "type" : "text"
                    },
                    {
                      "text" : "If ",
                      "type" : "text"
                    },
                    {
                      "code" : "head",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " is very small, ",
                      "type" : "text"
                    },
                    {
                      "code" : "tail",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " may even be zero, even though the",
                      "type" : "text"
                    },
                    {
                      "text" : " ",
                      "type" : "text"
                    },
                    {
                      "text" : "product is not exact.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "If ",
                      "type" : "text"
                    },
                    {
                      "code" : "head",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " is zero, ",
                      "type" : "text"
                    },
                    {
                      "code" : "tail",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " is also a zero with unspecified sign.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            }
          ],
          "type" : "unorderedList"
        },
        {
          "anchor" : "Postconditions:",
          "level" : 2,
          "text" : "Postconditions:",
          "type" : "heading"
        },
        {
          "items" : [
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "If ",
                      "type" : "text"
                    },
                    {
                      "code" : "head",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " is normal, then ",
                      "type" : "text"
                    },
                    {
                      "code" : "abs(tail) < head.ulp",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : ".",
                      "type" : "text"
                    },
                    {
                      "text" : " ",
                      "type" : "text"
                    },
                    {
                      "text" : "Assuming IEEE 754 default rounding, ",
                      "type" : "text"
                    },
                    {
                      "code" : "abs(tail) <= head.ulp\/2",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : ".",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "If both ",
                      "type" : "text"
                    },
                    {
                      "code" : "head",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " and ",
                      "type" : "text"
                    },
                    {
                      "code" : "tail",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " are normal, then ",
                      "type" : "text"
                    },
                    {
                      "code" : "a * b",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " is exactly",
                      "type" : "text"
                    },
                    {
                      "text" : " ",
                      "type" : "text"
                    },
                    {
                      "text" : "equal to ",
                      "type" : "text"
                    },
                    {
                      "code" : "head + tail",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " when computed as real numbers.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
