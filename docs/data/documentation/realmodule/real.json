        {
          "inlineContent" : [
            {
              "text" : "Types conforming to this protocol provide the arithmetic and utility",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "operations defined by the ",
              "type" : "text"
            },
            {
              "code" : "FloatingPoint",
              "type" : "codeVoice"
            },
            {
              "text" : " protocol, and provide all of the",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "math functions defined by the ",
